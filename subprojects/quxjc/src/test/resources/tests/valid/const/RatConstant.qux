rat BAR is 5.0
rat BAZ is -5.0

rat f(rat a):
    return a + FOO + 0.5

rat g(rat a):
    return a + 0.5

void main([str] args):
    print FOO
    print BAR
    print BAZ
    print FOO == BAR
    print FOO == BAZ
    print BAR == BAZ
