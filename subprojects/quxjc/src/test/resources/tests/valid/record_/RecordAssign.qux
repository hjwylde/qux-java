package valid.record_

{int x, str y} f({int x, str y} xs):
    xs.x = 3

    return xs

void main([str] args):
    xs = {x: 5, y: 'foo'}
    print xs.x
    print xs.y
    print f(xs).x
    print xs.x
    xs.y = 'bar'
    print xs
    xs.y = null
    print xs
    xs.x *= 3
    print xs
    xs.x += 0
    print xs
    xs.x = {y: {z: null}}
    print xs
    xs.x.y.a = null
    print xs

