package qux.lang

import qux.lang.Nat$nat

type Stack is {[T] data}

void Stack::clear():
    this.data = []

T|null Stack::peek():
    if |this| == 0:
        return null

    return this.data[|this| - 1]

T|null Stack::pop():
    if |this| == 0:
        return null

    t = peek()
    this.data = this.data[:|this| - 1]

    return t

nat Stack::_len():
    return |this.data|

void Stack::push(T t):
    this.data += [t]

